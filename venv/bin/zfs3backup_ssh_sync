#!/home/marco/workspaces/z3/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'zfs3backup','console_scripts','zfs3backup_ssh_sync'
__requires__ = 'zfs3backup'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('zfs3backup', 'console_scripts', 'zfs3backup_ssh_sync')()
    )
